<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Connect Google Fit Test Suite">
    <listeners>
        <listener class-name="org.vantagefit.testUtils.Listeners"/>
    </listeners>
    
    <test name="Connect Google Fit Tests">
        <classes>
            <class name="org.vantagefit.testCases.ConnectGoogleFitTests">
                <methods>
                    <include name="testConnectGoogleFitTitleDisplayed"/>
                    <include name="testGoogleFitDescriptionDisplayed"/>
                    <include name="testNavigateToPermissionActivityPage"/>
                    <include name="testNavigateToPermissionLocationPage"/>
                    <include name="testNavigateThroughPermissions"/>
                    <include name="testVisibilityOfAllowInPhysicalPermission"/>
                    <include name="testVisibilityOfDenyInPhysicalPermission"/>
                    <include name="testVisibilityOfAllowWhileUsingAppInLocationPermission"/>
                    <include name="testVisibilityOfAllowOnlyThisTimeInLocationPermission"/>
                    <include name="testVisibilityOfDenyInLocationPermission"/>
                    <include name="testClickSkipNavigatesToHomeWithNotificationPermission"/>
                </methods>
            </class>
        </classes>
    </test>
</suite> 